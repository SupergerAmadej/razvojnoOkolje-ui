<Window x:Class="Wpf_razvojnoOkolje.OgrodjeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_razvojnoOkolje"
        mc:Ignorable="d"
        Title="NewTip" Height="130" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="textBoxOgrodje" HorizontalAlignment="Left" Height="23" Margin="15,25,0,0" TextWrapping="Wrap" ToolTip="Naziv" VerticalAlignment="Top" Width="250"/>
        <Label x:Name="label" Content="Naziv Ogrodja" HorizontalAlignment="Left" Margin="15,4,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="Left" Margin="15,50,0,0" VerticalAlignment="Top" Width="75" Click="buttonOK_Click"/>

    </Grid>
</Window>
